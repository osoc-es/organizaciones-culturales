<%- include('partials/_header') %>

    <body>
        <nav class="navbar mb-5 " style="background-color: black !important; height: 5rem;">
            <img src="logo.png" class="mx-auto">
        </nav>

        <div class="card mx-auto" style="width: 25rem;">
            <div class="card-body">
            <h5 class="card-title" style="text-align: center; font-size: xx-large; font-weight: bold;">Bienvenido</h5>
            <br>

            <form action="/signupOrganizacion" method="POST">
                <div class="form-group">
                <label for="exampleInputnombre1">Nombre de la Organización</label>
                <input type="nombre" class="form-control" id="exampleInputnombre1" aria-describedby="emailHelp" name="name">
                </div>
                <div class="form-group">
                <label for="exampleInputcorreo1">Correo de contacto</label>
                <input type="correo" class="form-control" id="exampleInputcorreo1" name="email">
                </div>
                <div class="form-group">
                    <label for="exampleInputlocaliz1">Localización</label>
                    <input type="localiz" class="form-control" id="exampleInputlocaliz1" name="location">
                </div>
                <div class="form-group">
                    <label for="exampleInputweb1">Página web</label>
                    <input type="web" class="form-control" id="exampleInputweb1" name="webPage">
                </div>
                <div class="form-group">
                    <label for="exampleInputcontra1">Contraseña</label>
                    <input type="contra" class="form-control" id="exampleInputcontra1" name="password">
                </div>
                <button type="continuar" class="btn btn-light">Continuar</button> 
            </form>
            <br><br>

            <div class="card-text" style="text-align: center;">
                ¿Ya tienes una cuenta?
                <a href="#">Accede</a>
            </div>
        </div>

        <%- include('partials/_footer.ejs') %>